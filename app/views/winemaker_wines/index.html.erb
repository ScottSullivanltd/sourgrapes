<br>
<h1><%= @winemaker.name %> Wines</h1>
<% @wines.each do |wine| %>
  <h3><%= wine.name %></h3>
  <p>Blend: <%= wine.blend %></p>
  <p>Vintage: <%= wine.vintage %></p>
  <p>Barrels Produced: <%= wine.barrels_produced %></p>
  <p>Signature Label: <%= wine.signature_label %></p>
<% end %>
<br>

<p><%= link_to "Sort Wine Alphabetically", "/winemakers/#{@winemaker.id}/wines" %></p>

<%= link_to "Create Wine", "/winemakers/#{@winemaker.id}/wines/new" %>
