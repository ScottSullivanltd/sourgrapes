<%= form_with url: '/winemakers', method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :region %>
  <%= form.text_field :region %>
  <%= form.label :public_tasting, 'Public Tasting' %>
  <%= form.text_field :public_tasting %>
  <%= form.label :vineyard_acreage, 'Vineyard Acreage' %>
  <%= form.text_field :vineyard_acreage %>
  <%= form.submit 'Create Winemaker' %>
<% end %>
